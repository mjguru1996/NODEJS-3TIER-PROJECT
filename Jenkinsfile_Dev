pipeline {
    agent any

    stages{
        stage('GIT Checkout') {
            steps {
                git branch: 'dev', url: 'https://github.com/mjguru1996/Shack.git'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing...'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying...'
            }
        }
    }
    }
